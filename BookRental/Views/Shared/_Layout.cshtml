<!DOCTYPE html>
<html style="position:relative;min-height:100%;">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>	

<body>
	
		@Html.Partial("_PartialNavBar")

		<div class="container body-content">
			@RenderBody()
			<hr />

		</div>
		<footer style="position:fixed; bottom:0; width:100%;height:60px;background-color:#033B72;color:#fff;">
			<div style="padding-left:15px;padding-right:15px;"><span class="text-muted">Place for a footer</span></div>
				
		</footer>


	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bookRentalJs")
	@RenderSection("scripts", required: false)
</body>
</html>
